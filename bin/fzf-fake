#!/usr/bin/env php
<?php

$stream = fopen('php://stdin','r');
$input = explode(PHP_EOL, stream_get_contents($stream));

foreach ($argv as $arg) {
    switch ($arg) {
        case '--fake-first':
            echo $input[0];
            exit(0);
    }
}

echo implode(' ', $argv);
echo PHP_EOL;
echo implode(PHP_EOL, $input);
exit(0);
